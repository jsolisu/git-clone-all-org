{
  "name": "git-clone-all-org",
  "version": "1.2.20",
  "description": "Clone all organization branches from a Git Server",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "preferGlobal": true,
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "git"
  ],
  "author": "Jean C. Sol√≠s <jsolisu@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://jsolisu.visualstudio.com/git-clone-all-org/_git/git-clone-all-org"
  },
  "homepage": "https://jsolisu.visualstudio.com/git-clone-all-org/_git/git-clone-all-org",
  "dependencies": {
    "@octokit/rest": "^16.24.3",
    "@types/command-exists": "^1.2.0",
    "@types/rimraf": "^2.0.2",
    "@types/yargs": "^12.0.12",
    "azure-devops-node-api": "^7.2.0",
    "command-exists": "^1.2.8",
    "moment": "^2.24.0",
    "rimraf": "^2.6.3",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/node": "^11.13.5",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.3"
  }
}
