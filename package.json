{
  "name": "git-clone-all-org",
  "version": "1.2.22",
  "description": "Clone all organization branches from a Git Server",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "preferGlobal": true,
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "git"
  ],
  "author": "Jean C. Sol√≠s <jsolisu@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://jsolisu.visualstudio.com/git-clone-all-org/_git/git-clone-all-org"
  },
  "homepage": "https://jsolisu.visualstudio.com/git-clone-all-org/_git/git-clone-all-org",
  "dependencies": {
    "@octokit/rest": "^18.0.1",
    "@types/command-exists": "^1.2.0",
    "@types/prettier": "^2.0.2",
    "@types/rimraf": "^3.0.0",
    "@types/yargs": "^15.0.5",
    "azure-devops-node-api": "^10.1.1",
    "command-exists": "^1.2.9",
    "moment": "^2.27.0",
    "rimraf": "^3.0.2",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "prettier": "^2.0.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  }
}
