{
  "name": "git-clone-all-org",
  "version": "1.2.23",
  "description": "Clone all organization branches from a Git Server",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "preferGlobal": true,
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "git"
  ],
  "author": "Jean C. Sol√≠s <jsolisu@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://jsolisu.visualstudio.com/git-clone-all-org/_git/git-clone-all-org"
  },
  "homepage": "https://jsolisu.visualstudio.com/git-clone-all-org/_git/git-clone-all-org",
  "dependencies": {
    "@octokit/rest": "^21.1.0",
    "@types/command-exists": "^1.2.3",
    "@types/prettier": "^3.0.0",
    "@types/rimraf": "^4.0.5",
    "@types/yargs": "^17.0.33",
    "azure-devops-node-api": "^14.1.0",
    "command-exists": "^1.2.9",
    "moment": "^2.30.1",
    "rimraf": "^6.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "prettier": "^3.4.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.7.3"
  }
}
